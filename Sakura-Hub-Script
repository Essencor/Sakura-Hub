   end,
})

local Button = Tab:CreateButton({
   
   Name = "Simple Spy",
   Callback = function()
  loadstring(game:HttpGet("https://raw.githubusercontent.com/78n/SimpleSpy/main/SimpleSpyBeta.lua"))() -- The function that takes place when the button is pressed
   end,
})
local Divider = Tab:CreateDivider()
local Button = Tab:CreateButton({

   Name = "Rejoin Server",
   Callback = function()
  loadstring(game:HttpGet("https://pastebin.com/raw/DcnDzSX9"))() -- The function that takes place when the button is pressed
   end,
})

local Button = Tab:CreateButton({
   
   Name = "Serverhop", -- Serverhop
   Callback = function()
  local tp,hs,pid,jid=game:GetService("TeleportService"),game:GetService("HttpService"),game.PlaceId,game.JobId
local servers,cursor={},nil
repeat
    local res=hs:JSONDecode(game:HttpGet(("https://games.roblox.com/v1/games/%d/servers/Public?sortOrder=Asc&limit=100%s"):format(pid,cursor and "&cursor="..cursor or "")))
    for _,srv in ipairs(res.data) do
        if srv.id~=jid and srv.playing<srv.maxPlayers then
            table.insert(servers,srv.id)
        end
    end
    cursor=res.nextPageCursor
until not cursor
if #servers>0 then
    tp:TeleportToPlaceInstance(pid,servers[math.random(#servers)])
end
   end,
})
 -- End Of Serverhop Button
-- Notification
Rayfield:Notify({
   Title = "Welcome To Sakura!",
   Content = "Welcome to Sakura Hub! Please know that only 1 person is working on this.",
   Duration = 12,
   Image = "smile",
})

Window.ModifyTheme('Bloom') -- Handles Theme

